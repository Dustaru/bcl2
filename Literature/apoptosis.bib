	Mesh = {Apoptosis; Cell Line; Cytochromes c; Humans; Microscopy, Fluorescence; Mitochondria; Mitochondrial Membranes; Permeability; Pore Forming Cytotoxic Proteins; Protein Multimerization; bcl-2-Associated X Protein},
	Month = {Feb},
